<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BokningsSystemMaui.Views.TestPage"
             Title="TestPage">
    <VerticalStackLayout>
        <Label 
            x:Name="SessionId"
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Label 
            x:Name="SessionName"
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Label 
            x:Name="Password"
            Text="Welcome to .NET MAUI!"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button
            x:Name="BackButton"
            Text="Tillbaka"
            WidthRequest="500"
            Clicked="OnClickedBackButton"/>
    </VerticalStackLayout>
</ContentPage>